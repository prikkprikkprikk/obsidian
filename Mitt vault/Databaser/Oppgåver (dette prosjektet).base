filters:
  or:
    - file.inFolder("Databaser/Oppgaver")
views:
  - type: table
    name: Dette prosjektet
    filters:
      and:
        - Prosjekt.contains(this.file)
    order:
      - file.name
      - Oppgavestatus
    sort:
      - property: Oppgavestatus
        direction: ASC
    columnSize:
      file.name: 416
