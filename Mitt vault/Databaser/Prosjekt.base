filters:
  or:
    - file.inFolder("Databaser/Prosjekt")
views:
  - type: table
    name: Alle prosjekter
    order:
      - file.name
    columns:
      - file.name
      - file.folder
      - file.frontmatter.status
      - file.frontmatter.omr책de
      - file.frontmatter.opprettet
  - type: table
    name: Aktive prosjekter
    filters:
      or:
        - Prosjektstatus == "I arbeid"
    order:
      - file.frontmatter.prioritet
      - file.name
      - Prosjektstatus
    columns:
      - file.name
      - file.frontmatter.status
      - file.frontmatter.omr책de
      - file.frontmatter.overleveringsfrist
  - type: table
    name: CoreTrek-prosjekt
    filters:
      and:
        - Prosjekttype == "CoreTrek"
    groupBy:
      property: Prosjektstatus
      direction: ASC
    order:
      - file.frontmatter.omr책de
      - file.name
      - Prosjektstatus
    columns:
      - file.name
      - file.frontmatter.status
      - file.frontmatter.omr책de
    cardSize: 320
