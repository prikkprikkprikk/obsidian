views:
  - type: table
    name: Table
    filters:
      and:
        - file.folder == "Oppgaver"
    order:
      - file.name
      - Prosjekt
      - Hvem
  - type: table
    name: View
    filters:
      and:
        - file.folder == "Bagerens vei 17"
        - file.ext != ".base"
    order:
      - file.name
      - Prosjekt
      - Hovedprosjekt
    rowHeight: tall
